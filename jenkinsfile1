@Library('my-library-lab@1.0') _

pipeline {
    agent any
    stages {
        stage('git checkout') {
            steps {
                script {
                    gitCheckout(
                        branch: 'main',
                        url: 'https://github.com/bdino2005/devops_java_app.git'
                    )
                }
            }
        }
         stage('unit test maven') {
            steps {
                script {
                    mvnTest()
                }
            }
        }
    }
}






